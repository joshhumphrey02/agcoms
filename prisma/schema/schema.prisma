generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["relationJoins", "tracing", "prismaSchemaFolder", "views", "postgresqlExtensions"]
}

datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
  extensions = [postgis]
}

model Message {
  id          String  @id @default(cuid())
  fullName    String
  email       String
  town        String
  message     String
  phone       String
  intlPhone   String? @unique
  country     String?
  countryCode String?
}
